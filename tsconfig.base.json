{
  "compilerOptions": {
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "incremental": true,
    "target": "es2022",
    "lib": ["es2023"]
  },
  "exclude": ["node_modules", "dist"]
}
